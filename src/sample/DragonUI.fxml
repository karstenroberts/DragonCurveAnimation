<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.scene.canvas.Canvas?>
<?import javafx.scene.control.Button?>
<?import javafx.scene.control.TextField?>
<?import javafx.scene.layout.Pane?>

<Pane xmlns="http://javafx.com/javafx/8.0.141" xmlns:fx="http://javafx.com/fxml/1" fx:controller="sample.Controller">
   <children>
      <Canvas fx:id="canvas" height="1000.0" width="1700.0" />
      <Canvas fx:id="animationCanvas" height="1000.0" width="1700.0" />
      <TextField fx:id="iterationsField" layoutX="1634.0" layoutY="14.0" onAction="#drawCurrCurve" prefHeight="33.0" prefWidth="52.0" />
      <Button fx:id="drawButton" layoutX="1569.0" layoutY="17.0" mnemonicParsing="false" onAction="#drawCurrCurve" text="Draw" />
      <Button layoutX="1571.0" layoutY="58.0" mnemonicParsing="false" onAction="#prevLevel" text="&lt;" />
      <Button layoutX="1660.0" layoutY="58.0" mnemonicParsing="false" onAction="#nextLevel" text="&gt;" />
      <Button layoutX="1601.0" layoutY="58.0" mnemonicParsing="false" onAction="#pausePlay" text="Pause" />
   </children>
</Pane>
